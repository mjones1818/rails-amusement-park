  <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>

<%= form_for :user, url: '/signin' do |f|  %>
  <%= f.label "Your name" %>
  <%= f.collection_select :name, @users, :name, :name %> <br>
  <%= f.label :password %>
  <%= f.password_field :password%> <br>
  <%= f.submit "Sign In"%>
<% end %>